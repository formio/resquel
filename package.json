{
  "name": "resquel",
  "version": "2.0.0",
  "description": "Easily convert a SQL database into a REST API using Express.js.",
  "main": "./lib/resquel.js",
  "types": "./lib/resquel.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib",
    "clean:build": "yarn clean; yarn build",
    "build": "tsc",
    "start:mysql": "ts-node example/mysql/server.ts",
    "start:mssql": "ts-node example/mssql/server.ts",
    "start:postgresql": "ts-node example/postgresql/server.ts",
    "test": "yarn test:mysql; yarn test:mssql; yarn test:postgresql",
    "test:mysql": "mocha -r ts-node/register -t 20000 -b test/mysql.ts",
    "test:mssql": "mocha -r ts-node/register -t 20000 -b test/mssql.ts",
    "test:postgresql": "mocha -r ts-node/register -t 20000 -b test/postgresql.ts"
  },
  "author": "form.io",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.5",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.168",
    "@types/method-override": "^0.0.31",
    "@types/mocha": "^8.2.0",
    "@types/mssql": "^6.0.7",
    "@types/mysql": "^2.15.17",
    "@types/node": "^14.14.22",
    "@types/pg": "^7.14.9",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.15.0",
    "debug": "^2.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^6.10.0",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "knex": "^0.21.16",
    "lodash": "^4.11.1",
    "method-override": "^2.3.5",
    "mocha": "^3.0.2",
    "mssql": "^6.3.1",
    "mysql": "^2.18.1",
    "pg": "^8.5.1",
    "prettier": "^2.2.1",
    "supertest": "^2.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {}
}
