    "@types/mysql": "^2.15.17",
    "@types/node": "^14.14.22",
    "@types/pg": "^7.14.9",
    "@types/supertest": "^2.0.10",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "faker": "^5.1.0",
    "mocha": "^3.0.2",
    "prettier": "^1.19.1",
    "supertest": "^2.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
