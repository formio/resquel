{
  "name": "resquel",
  "version": "2.0.0",
  "description": "Easily convert a SQL database into a REST API using Express.js.",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "npm run test:mysql; npm run test:mssql; npm run test:postgresql",
    "test:mysql": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register -t 20000 -b test/mysql.ts",
    "test:mssql": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register -t 20000 -b test/mssql.ts",
    "test:postgresql": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register -t 20000 -b test/postgresql.ts",
    "lint": "eslint ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.15.0",
    "debug": "^2.2.0",
    "express": "^4.17.1",
    "lodash": "^4.11.1",
    "method-override": "^2.3.5",
    "mssql": "^6.3.1",
    "mysql": "^2.18.1",
    "pg": "^6.1.0",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chance": "^1.1.1",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.5",
    "@types/lodash": "^4.14.168",
    "@types/method-override": "^0.0.31",
    "@types/mocha": "^8.2.0",
    "@types/mssql": "^6.0.7",
    "@types/mysql": "^2.15.17",
    "@types/node": "^14.14.22",
    "@types/pg": "^7.14.9",
    "@types/supertest": "^2.0.10",
    "chance": "^1.0.4",
    "eslint": "^3.2.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "faker": "^5.1.0",
    "mocha": "^3.0.2",
    "prettier": "^1.19.1",
    "supertest": "^2.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
