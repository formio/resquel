{
  "name": "resquel",
  "version": "2.0.0",
  "description": "Easily convert a SQL database into a REST API using Express.js.",
  "main": "src/index.ts",
  "scripts": {
    "start:mysql": "ts-node example/mysql/server.ts",
    "start:mssql": "ts-node example/mssql/server.ts",
    "start:postgresql": "ts-node example/postgresql/server.ts",
    "test": "npm run test:mysql; npm run test:mssql; npm run test:postgresql",
    "test:mysql": "mocha -r ts-node/register -t 20000 -b test/mysql.ts",
    "test:mssql": "mocha -r ts-node/register -t 20000 -b test/mssql.ts",
    "test:postgresql": "mocha -r ts-node/register -t 20000 -b test/postgresql.ts"
  },
  "author": "form.io",
  "license": "ISC",
  "dependencies": {
    "basic-auth-connect": "^1.0.0",
