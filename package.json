    "debug": "^2.2.0",
    "express": "^4.17.1",
    "lodash": "^4.11.1",
    "method-override": "^2.3.5",
    "mssql": "^6.3.1",
    "mysql": "^2.18.1",
    "pg": "^8.5.1",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chance": "^1.1.1",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.5",
    "@types/lodash": "^4.14.168",
    "@types/method-override": "^0.0.31",
